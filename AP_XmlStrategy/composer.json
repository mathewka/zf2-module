    "authors": [
        {
            "name": "Alessandro Pietrobelli",
            "email": "alessandro.pietrobelli@gmail.com",
            "homepage": "http://alessandropietrobelli.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": ">=2.0.4"
    },
    "autoload": {
        "psr-0": {
            "AP_XmlStrategy": "src/"
        },
        "classmap": [
            "./Module.php"
